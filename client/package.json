{
  "name": "red-tetris",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:unitary": "react-scripts test",
    "test:coverage": "react-scripts test --env=jsdom --coverage  CI=true",
    "test": "react-scripts test --env=jsdom --coverage  CI=true",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/hooks/**",
      "!src/components/**/__snapshots__/**",
      "!src/store/middleware.js",
      "!src/components/Loader.js",
      "!src/index.js",
      "!src/views/Game/Game.js"
    ]
  }
  
}
